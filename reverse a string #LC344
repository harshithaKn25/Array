2.reverse a string   #LC 344
   Input=[“h”,”e”,”l”,”l”,”o”]
Output=[“o”,”l”,”l”,”e”,”h”]
-----------------------------------
void reverseString(char* s, int sSize)
 {
    int j=sSize-1;
    char temp;
  for(int i=0;i<j;)
  {
     temp=s[i];
     s[i]=s[j];
     s[j]=temp;
     i++;
     j--;
  }  
}
